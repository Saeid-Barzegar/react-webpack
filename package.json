{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "React JS boilerplate created with Webpack ",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack server --config config/webpack.config.dev.js --hot",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.config.prod.js",
    "typecheck": "tsc --noEmit",
